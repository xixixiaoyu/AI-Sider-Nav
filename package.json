{
  "name": "ai-sider-nav",
  "version": "1.0.0",
  "description": "AI-powered browser extension with navigation and sidebar features",
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "vue-tsc && vite build",
    "build:zip": "npm run build && node scripts/build.js --zip",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "icons": "node scripts/generate-icons.js",
    "validate": "node scripts/validate-manifest.js",
    "check-size": "node scripts/check-size.js",
    "prebuild": "npm run validate",
    "postbuild": "npm run check-size"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "pinia": "^2.1.7",
    "@vueuse/core": "^10.7.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.0",
    "@iconify-json/mdi": "^1.1.0",
    "@rushstack/eslint-patch": "^1.6.0",
    "@types/node": "^20.10.0",
    "@types/webextension-polyfill": "^0.10.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@unocss/eslint-config": "^0.58.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.0",
    "archiver": "^6.0.0",
    "chokidar": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "unocss": "^0.58.0",
    "vite": "^5.0.0",
    "vite-plugin-web-extension": "^4.1.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.0"
  },
  "keywords": [
    "browser-extension",
    "chrome-extension",
    "edge-extension",
    "ai",
    "sidebar",
    "navigation",
    "vue3",
    "typescript"
  ],
  "author": "AI Sider Nav Team",
  "license": "MIT"
}
